#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int isprime(int n)
{
    if(n==1)
        return 0;
    for(int i=2; i<=sqrt(n); i++)
    {
        if(n%i==0)
            return 0;
    }
    return 1;
}
void print_coordinate(int n)
{
    int x=0,y=0,sc=1,sl=2,a=1;
    for(int i=1;i<=n;i++)
    {
        if(sc<=sl/2)
            x=x+a;
        else
            y=y+a;
        if(sc==sl)
        {
            sc=0;
            sl=sl+2;
            a=-a;
        }
        sc++;
    }
    printf("%d %d\n",x,y);
}

int main()
{
    int a[100000],i=1;
    a[0]=2;
    for(int j=3;i<100000;j=j+2)
    {
        if(isprime(j))
        {
            a[i]=j;
            i++;
        }

    }
    int t;
    scanf("%d",&t);
    while(t--)
    {
        int x;
        scanf("%d",&x);
        for(i=0;a[i]!=x;i++);
        print_coordinate(i);
    }


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
